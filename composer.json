    "require": {
        "php": "^8.4",
        "aws/aws-sdk-php": "^3.366"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "staticforge": {
            "feature": "Calevans\\StaticForgeS3\\Feature"
        }
    }
}