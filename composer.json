    "description": "StaticForge Feature: S3MediaOffload",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Calevans\\StaticForgeS3\\": "src/"
        }
    },
    "autoload-dev": {
